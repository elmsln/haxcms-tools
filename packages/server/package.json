{
  "name": "@haxcms/server",
  "description": "Node server for HAXcms sites.",
  "version": "0.0.1",
  "author": "heyMP @heyMP",
  "bin": {
    "hax-server": "./bin/run"
  },
  "bugs": "https://github.com/elmsln/hax-tools/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "apollo-server": "^2.4.8",
    "bcrypt": "^3.0.6",
    "browser-sync": "^2.26.4",
    "dotenv": "^7.0.0",
    "graphql": "^14.2.1",
    "graphql-type-json": "^0.3.0",
    "jsonwebtoken": "^8.5.1",
    "polymer-cli": "^1.9.8"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^8",
    "mocha": "^5",
    "nodemon": "^1.18.11",
    "nyc": "^13"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/elmsln/hax-tools",
  "keywords": [
    "oclif"
  ],
  "license": "Apache 2.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "hax-server",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "elmsln/hax-tools",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "dev": "nodemon ./src/server/index.js"
  }
}
