{
  "name": "@haxcms/server",
  "description": "Node server for HAXcms sites.",
  "version": "0.0.3",
  "author": "heyMP @heyMP",
  "bin": {
    "hax-server": "./bin/run"
  },
  "bugs": "https://github.com/elmsln/hax-tools/issues",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "jsonwebtoken": "^8.5.1",
    "@11ty/eleventy": "0.10.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "gray-matter": "^4.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^8",
    "mocha": "^5",
    "nodemon": "^1.18.11",
    "nyc": "^13"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/elmsln/hax-tools",
  "keywords": [
    "oclif"
  ],
  "license": "Apache 2.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "hax-server",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "elmsln/hax-tools",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "dev": "nodemon ./src/server/index.js"
  },
  "gitHead": "219c8aba21c02729d486884b714d1a78afd41251"
}
