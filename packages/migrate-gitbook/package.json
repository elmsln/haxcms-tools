{
  "name": "@haxcms/migrate-gitbook",
  "description": "Migrate a Gitbook site to HAXcms.",
  "version": "0.0.7",
  "author": "heyMP <mgp140@gmail.com>",
  "bin": {
    "migrate-gitbook": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/elmsln/haxcms-tools/issues"
  },
  "dependencies": {
    "@desertnet/scanner": "^1.1.1",
    "@haxcms/gitbook-2-outline-schema": "^0.0.7",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-plugins": "^1.7.7",
    "batch": "^0.6.1",
    "cli-progress": "^2.1.1",
    "fs-extra": "^7.0.1",
    "hooks": "^0.3.2",
    "markdown-it": "^8.4.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "lib"
  ],
  "homepage": "https://github.com/elmsln/haxcms-tools#readme",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "type": "core",
    "bin": "migrate-gitbook",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "hooks": {
      "run-convert-item-post": "./src/hooks/run-convert-item-post/example"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elmsln/haxcms-tools.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "gitHead": "219c8aba21c02729d486884b714d1a78afd41251"
}
